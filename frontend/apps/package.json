    "axios": "^0.21.1",
    "axios-case-converter": "^0.8.1",
    "camel-case": "^4.1.2",
    "connected-react-router": "^6.9.1",
    "keycloak-js": "^12.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "web-vitals": "^1.1.1"
  },
